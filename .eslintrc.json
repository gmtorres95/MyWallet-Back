{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [
            "error",
            {
                "js": "ignorePackages"
            }
        ],
        "linebreak-style": 0,
        "consistent-return": "off",
        "import/prefer-default-export": "off",
        "no-console": "off"
    }
}
